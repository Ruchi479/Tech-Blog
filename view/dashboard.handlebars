 <div class='container-fluid'>
            <section>
                <div class='d-flex flex-row-reverse col-12'>
                    <button class='btn btn-secondary' id='btn-add'>+ Add Post</button>
                </div>
            </section>
            
            <section class='article-list'>
                <div class='col-12'>
                        <h3 class='my-articles'>My Posts</h3>
                        <table class='table table-striped table-bordered table-hover tablewrap' width='100%' aria-label='List of Articles You Have Posted, with Option to Edit or Delete'>
                            <thead class='thead-dark'>
                                <tr>
                                    <th scope='col' style='width: 20%'>Title</th>
                                    <th scope='col' style='width: 35%'>Content</th>
                                    <th scope='col' style='width: 10%'>Created</th>
                                    <th scope='col' style='width: 10%'>Last Updated</th>
                                    <th scope='col' style='width: 25%'>Edit</th>
                                </tr>
                            </thead>
                    {{#if posts.length}}
                        {{#each posts as |post|}}
                        <tbody>
                            <tr><a href='/post/{{post.id}}'></a>
                                <td scope='row'>{{post.title}}</th>
                                <td id='content' class='word-break: break-word;'>{{post.content}}</td>
                                <td id='created-at'>{{format_date post.created_at}}</td>
                                <td id='updated_at'>{{format_date post.updated_at}}</td>
                                <td class='edit-btns'>
                                    <button class='btn btn-primary btn-update' data-id='{{post.id}}'>Update</button>
                                    <button class='btn btn-warning btn-delete' data-id='{{post.id}}'>Delete</button>
                                </td>
                            </tr>
                        </tbody>
                        {{/each}}
                    {{/if}}
                </div>
            </section>
        </div>

<script src='./js/dashboard.js'></script>